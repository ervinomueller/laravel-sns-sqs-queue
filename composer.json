{
    "name": "maxgaurav/laravel-sqs-sns",
    "description": "Sns Messages as queues using extended laravel sqs driver.",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "homepage": "https://github.com/maxgaurav/laravel-sqs-sns",
    "authors": [
        {
            "name": "Max Gaurav",
            "email": "max.gaurav@ithands.net"
        }
    ],
    "keywords": [
        "sns",
        "sqs",
        "laravel",
        "queue",
        "sns-sqs",
        "aws"
    ],
    "require": {
        "php" : "^7.2",
        "illuminate/queue": "6.*",
        "aws/aws-sdk-php": "^3.62",
        "ext-json": "*"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MaxGaurav\\LaravelSnsSqsQueue\\SnsSqsServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "MaxGaurav\\LaravelSnsSqsQueue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    }
}
